// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  email String @unique
  password String
}

model Company{
  id String  @id @default(uuid())
  registerd DateTime @default(now())
  title String
  email String @unique
  number BigInt
  status Boolean
  hash String
}